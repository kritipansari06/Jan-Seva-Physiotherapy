{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/videos/(.*)\\.(mp4|MP4)",
      "headers": [
        { "key": "Content-Type", "value": "video/mp4" },
        { "key": "Accept-Ranges", "value": "bytes" }
      ]
    },
    {
      "source": "/videos/(.*)\\.(mov|MOV)",
      "headers": [
        { "key": "Content-Type", "value": "video/quicktime" },
        { "key": "Accept-Ranges", "value": "bytes" }
      ]
    }
  ]
}